    {
        "id": "b984ed503db054e8",
        "type": "function",
        "z": "bcb7bae54df0102d",
        "name": "Temperature Function",
        "func": "// Lấy giá trị nhiệt độ từ payload\nlet temperature = parseFloat(msg.payload);\n\n// Kiểm tra chế độ hiện tại (tự động hay thủ công)\nconst controlMode = flow.get(\"controlMode\") || \"manual\";\n\n// Chỉ xử lý nhiệt độ nếu đang ở chế độ tự động\nif (controlMode === \"auto\") {\n    // Ngưỡng nhiệt độ để bật quạt\n    const threshold = 32;\n    \n    // Kiểm tra nhiệt độ và quyết định bật/tắt relay\n    if (temperature > threshold) {\n        msg.payload = false;  // Bật quạt (relay OFF trong trạng thái NC)\n    } else {\n        msg.payload = true; // Tắt quạt (relay ON trong trạng thái NC)\n    }\n    \n    // Gửi lệnh đến topic điều khiển relay\n    msg.topic = \"esp32/led1\";\n    return msg;\n} else {\n    // Ở chế độ thủ công, không làm gì cả, để các nút điều khiển thủ công xử lý\n    return null;\n}",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 740,
        "y": 960,
        "wires": [
            [
                "f84ceabc0e2c2083"
            ]
        ]
    },
    {
        "id": "1a4e6abb13f8f6c3",
        "type": "function",
        "z": "bcb7bae54df0102d",
        "name": "Soil Function",
        "func": "// Lấy giá trị độ ẩm đất từ payload\nlet soilMoisture = parseFloat(msg.payload);\n\n// Kiểm tra chế độ hiện tại (tự động hay thủ công)\nconst controlMode = flow.get(\"controlMode\") || \"manual\";\n\n// Chỉ xử lý nếu đang ở chế độ tự động\nif (controlMode === \"auto\") {\n    // Ngưỡng độ ẩm đất để bật bơm\n    const threshold = 2000; // Điều chỉnh theo nhu cầu của bạn\n    \n    // Kiểm tra độ ẩm đất và quyết định bật/tắt bơm\n    if (soilMoisture > threshold) { // Đất khô\n        msg.payload = false;  // Bật bơm\n    } else {                  // Đất đủ ẩm\n        msg.payload = true;   // Tắt bơm\n    }\n    \n    // Gửi lệnh đến topic điều khiển bơm\n    msg.topic = \"esp32/led2\";\n    return msg;\n} else {\n    // Ở chế độ thủ công, không làm gì cả\n    return null;\n}",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 770,
        "y": 920,
        "wires": [
            [
                "f84ceabc0e2c2083"
            ]
        ]
    },
    {
        "id": "828ea5e72e187de9",
        "type": "function",
        "z": "bcb7bae54df0102d",
        "name": "Light Function",
        "func": "// Lấy giá trị ánh sáng từ payload\nlet lightLevel = parseFloat(msg.payload);\n\n// Kiểm tra chế độ hiện tại (tự động hay thủ công)\nconst controlMode = flow.get(\"controlMode\") || \"manual\";\n\n// Chỉ xử lý nếu đang ở chế độ tự động\nif (controlMode === \"auto\") {\n    // Ngưỡng ánh sáng để bật/tắt đèn\n    const lightThreshold = 300; // Điều chỉnh theo nhu cầu\n    \n    // Tạo thông điệp điều khiển đèn\n    let lightMsg = {\n        topic: \"esp32/led3\",\n        payload: lightLevel < lightThreshold ? \"true\" : \"false\" // Bật khi trời tối, tắt khi sáng\n    };\n\n    // Trả về thông điệp điều khiển đèn\n    return lightMsg;\n} else {\n    // Ở chế độ thủ công, không làm gì cả\n    return null;\n}\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
